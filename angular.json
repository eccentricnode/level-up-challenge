{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "pokemon": {
      "root": "apps/pokemon/",
      "sourceRoot": "apps/pokemon/src",
      "projectType": "application",
      "prefix": "level",
      "schematics": {
        "@nrwl/schematics:component": {
          "styleext": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/pokemon",
            "index": "apps/pokemon/src/index.html",
            "main": "apps/pokemon/src/main.ts",
            "polyfills": "apps/pokemon/src/polyfills.ts",
            "tsConfig": "apps/pokemon/tsconfig.app.json",
            "assets": [
              "apps/pokemon/src/favicon.ico",
              "apps/pokemon/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/pink-bluegrey.css",
              "apps/pokemon/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/pokemon/src/environments/environment.ts",
                  "with": "apps/pokemon/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "pokemon:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "pokemon:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "pokemon:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/pokemon/src/test.ts",
            "polyfills": "apps/pokemon/src/polyfills.ts",
            "tsConfig": "apps/pokemon/tsconfig.spec.json",
            "karmaConfig": "apps/pokemon/karma.conf.js",
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/pink-bluegrey.css",
              "apps/pokemon/src/styles.scss"
            ],
            "scripts": [],
            "assets": [
              "apps/pokemon/src/favicon.ico",
              "apps/pokemon/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/pokemon/tsconfig.app.json",
              "apps/pokemon/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
